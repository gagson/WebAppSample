        <url-pattern>/admin/receive_changeStatus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReceiveRemoveUser</servlet-name>
        <url-pattern>/admin/receive_removeUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReceiveRemoveImage</servlet-name>
        <url-pattern>/user/receive_removeImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReceivePublicDownload</servlet-name>
        <url-pattern>/public_download</url-pattern>
    </servlet-mapping>
