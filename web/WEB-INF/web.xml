        <servlet-class>util.photoSearchResults</servlet-class>
        <multipart-config/>
    </servlet>
    <servlet>
        <servlet-name>readRequests</servlet-name>
        <servlet-class>user.readRequests</servlet-class>
        <multipart-config/>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>util.LogoutServlet</servlet-class>
        <multipart-config/>
    </servlet>
    <servlet>
        <servlet-name>ReceiveFileServlet</servlet-name>
        <servlet-class>user.ReceiveFileServlet</servlet-class>
        <multipart-config/>
    </servlet>
    <servlet>
        <servlet-name>EditPermission</servlet-name>
        <servlet-class>user.EditPermission</servlet-class>
        <multipart-config/>
    </servlet>
    <servlet>
        <servlet-name>ReceiveDownloadForm</servlet-name>
        <servlet-class>user.ReceiveDownloadForm</servlet-class>
        <multipart-config/>
    </servlet>
    <servlet>
        <servlet-name>ReceiveEditKeyword</servlet-name>
        <servlet-class>user.ReceiveEditKeyword</servlet-class>
        <multipart-config/>
    </servlet>
    <servlet>
        <servlet-name>ReceiveEditPermission</servlet-name>
        <servlet-class>user.ReceiveEditPermission</servlet-class>
        <multipart-config/>
    </servlet>
    <servlet>
        <servlet-name>ReceiveAdminForm</servlet-name>
        <servlet-class>admin.ReceiveAdminForm</servlet-class>
        <multipart-config/>
    </servlet>
    <servlet>
        <servlet-name>ReceiveAdminAddUser</servlet-name>
        <servlet-class>admin.ReceiveAdminAddUser</servlet-class>
        <multipart-config/>
    </servlet>
    <servlet>
        <servlet-name>ReceiveChangeStatus</servlet-name>
        <servlet-class>admin.ReceiveChangeStatus</servlet-class>
        <multipart-config/>
    </servlet>
    <servlet>
        <servlet-name>ReceiveRemoveUser</servlet-name>
        <servlet-class>admin.ReceiveRemoveUser</servlet-class>
        <multipart-config/>
    </servlet>
    <servlet>
        <servlet-name>ReceiveRemoveImage</servlet-name>
        <servlet-class>user.ReceiveRemoveImage</servlet-class>
        <multipart-config/>
    </servlet>
    <servlet>
        <servlet-name>ReceivePublicDownload</servlet-name>
        <servlet-class>util.ReceivePublicDownload</servlet-class>
        <multipart-config/>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminDashboard</servlet-name>
        <url-pattern>/admin/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserDashboard</servlet-name>
        <url-pattern>/user/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>upload</servlet-name>
        <url-pattern>/user/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Validate</servlet-name>
        <url-pattern>/validate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditPhotoKeyword</servlet-name>
        <url-pattern>/user/edit</url-pattern>
        <multipart-config/>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>photoSearch</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>photoSearchResults</servlet-name>
        <url-pattern>/search_results</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>readRequests</servlet-name>
